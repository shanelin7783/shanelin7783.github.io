{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "sourceMap": true,
      "noImplicitAny": false,
      "allowSyntheticDefaultImports": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "esModuleInterop": true,
      "declaration": false,
      "lib": ["es5", "es6", "dom", "scripthost"],
      "jsx": "react",
      "skipLibCheck": false,
      "pretty": true,
      "rootDir": "src",
      "baseUrl": ".",
      "paths": {
        "@components/*": ["./components/*"],
        "@pages/*": ["./pages/*"],
        "@core/*": ["./core/*"],
        "@static/*": ["./static/*"]
      }
    },
    "exclude": [
      "script",
      "static",
      ".storybook",
      ".vscode",
      "config",
      ".awcache"
    ]
  }
  